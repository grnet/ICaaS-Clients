<div class="small-6 large-5 columns sidebar">
    <ul class="no-bullet">
       {{#each queryResults as |build|}}
            {{display-build build=build action="deleteBuild"}}
            {{else}}
                <li class="empty-list">
                    No builds
                </li>
        {{/each}}
    </ul>
</div>
<div class="small-6 large-7 columns main">
    {{partial "create-build"}}
    {{outlet}}
</div>
