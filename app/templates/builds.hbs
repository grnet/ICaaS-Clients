<div class="middle">
    <div class="small-6 large-5 columns sidebar">
        <ul class="no-bullet">
           {{#each controller.queryResults as |build|}}
                <li>
                    <a {{action "deleteBuild" build}} class="right remove-sign" title="remove record">
                        {{fa-icon "fa-times"}}
                    </a>
                    {{#link-to "build" build.id}}
                        <i class="icon-disk-outline left"></i>
                        <i class="icon-disk-filled left"></i>
                        <span class="wrap-text name">{{build.name}}</span>
                    {{/link-to}}
                </li>
            {{/each}}
        </ul>
    </div>
    <div class="small-6 large-7 columns main">
        {{partial "create-build"}}
        {{outlet}}
    </div>
</div>
